/**index.wxss**/
.buyList{
  width: 100%;
  padding: 15px 20px 15px 145px;
  position: relative;
  border-bottom:1px solid #eee;
  height: 110px;
  font-size: 13px;
  color:#868595;
  box-sizing: border-box;
  -moz-box-sizing: border-box;
  /* Firefox */
  -webkit-box-sizing: border-box;
  /* Safari */
}
.buyList .peopleImg{
  width: 110px;
  height: 80px;
  position: absolute;
  top: 15px;
  left: 20px;
}
.buyList h3{
  font-size: 16px;
  display: block;
  margin: 0 0 5px 0;
  overflow: hidden;
  white-space: nowrap;
  text-overflow:ellipsis;
}
.buyList h5{
  font-weight: normal;
  color: #868595;
  font-size: 13px;
  margin-top: 20px;
}
.buyList small{
  color: #868595;
  font-size: 12px;
  position: absolute;
  bottom: 15px;
  right: 20px;
}